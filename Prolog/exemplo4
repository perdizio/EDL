fibonacci(1,M):- M = 1.
fibonacci(2,M):- M = 1.
fibonacci(N, MM):- N1 is N - 1,
    N2 is N - 2,
    fibonacci(N1, M1), 
    fibonacci(N2, M2), 
    MM is M1 + M2.

entrada2 :-
nl,
    write('Insira o valor de N da Sequencia de Fibonacci: '),nl,
    read(N),
    fibonacci(N, MM),nl,
    write('Valor do termo de posição N = '), write(N),
    write(' é: '), write(MM).
